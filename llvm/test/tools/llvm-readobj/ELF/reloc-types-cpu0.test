## Test that llvm-readobj/llvm-readelf shows proper relocation type
## names and values for cpu0 target.

# RUN: yaml2obj %s -o %t-cpu0.o
# RUN: llvm-readobj -r --expand-relocs %t-cpu0.o | FileCheck %s

# CHECK: Type: R_CPU0_NONE (0)
# CHECK: Type: R_CPU0_32 (2)
# CHECK: Type: R_CPU0_HI16 (5)
# CHECK: Type: R_CPU0_LO16 (6)
# CHECK: Type: R_CPU0_GPREL16 (7)
# CHECK: Type: R_CPU0_LITERAL (8)
# CHECK: Type: R_CPU0_GOT16 (9)
# CHECK: Type: R_CPU0_PC16 (10)
# CHECK: Type: R_CPU0_CALL16 (11)
# CHECK: Type: R_CPU0_GPREL32 (12)
# CHECK: Type: R_CPU0_PC24 (13)
# CHECK: Type: R_CPU0_GOT_HI16 (22)
# CHECK: Type: R_CPU0_GOT_LO16 (23)
# CHECK: Type: R_CPU0_RELGOT (36)
# CHECK: Type: R_CPU0_TLS_GD (42)
# CHECK: Type: R_CPU0_TLS_LDM (43)
# CHECK: Type: R_CPU0_TLS_DTP_HI16 (44)
# CHECK: Type: R_CPU0_TLS_DTP_LO16 (45)
# CHECK: Type: R_CPU0_TLS_GOTTPREL (46)
# CHECK: Type: R_CPU0_TLS_TPREL32 (47)
# CHECK: Type: R_CPU0_TLS_TP_HI16 (49)
# CHECK: Type: R_CPU0_TLS_TP_LO16 (50)
# CHECK: Type: R_CPU0_GLOB_DAT (51)
# CHECK: Type: R_CPU0_JUMP_SLOT (127)

--- !ELF
FileHeader:
  Class:   ELFCLASS32
  Data:    ELFDATA2LSB
  Type:    ET_REL
  Machine: EM_CPU0
Sections:
  - Name: .rela.text
    Type: SHT_RELA
    Relocations:
      - Type: R_CPU0_NONE
      - Type: R_CPU0_32
      - Type: R_CPU0_HI16
      - Type: R_CPU0_LO16
      - Type: R_CPU0_GPREL16
      - Type: R_CPU0_LITERAL
      - Type: R_CPU0_GOT16
      - Type: R_CPU0_PC16
      - Type: R_CPU0_CALL16
      - Type: R_CPU0_GPREL32
      - Type: R_CPU0_PC24
      - Type: R_CPU0_GOT_HI16
      - Type: R_CPU0_GOT_LO16
      - Type: R_CPU0_RELGOT
      - Type: R_CPU0_TLS_GD
      - Type: R_CPU0_TLS_LDM
      - Type: R_CPU0_TLS_DTP_HI16
      - Type: R_CPU0_TLS_DTP_LO16
      - Type: R_CPU0_TLS_GOTTPREL
      - Type: R_CPU0_TLS_TPREL32
      - Type: R_CPU0_TLS_TP_HI16
      - Type: R_CPU0_TLS_TP_LO16
      - Type: R_CPU0_GLOB_DAT
      - Type: R_CPU0_JUMP_SLOT
